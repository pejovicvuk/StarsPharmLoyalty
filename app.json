{
  "expo": {
    "name": "Stars Pharm Loyalty",
    "slug": "StarsPharmLoyalty",
    "scheme": "starspharmloyalty",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "description": "Stars Pharm Loyalty app for managing pharmacy loyalty program and rewards",
    "primaryColor": "#007AFF",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "This app accesses photo library to select images for shop items."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.starspharm.loyalty",
      "buildNumber": "2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app uses camera to scan QR codes and receipts for loyalty program functionality.",
        "NSPhotoLibraryUsageDescription": "This app accesses photo library to select images for shop items."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.starspharm.loyalty"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "70c301b1-d4d7-4ad7-ac92-e4537b3cedfb"
      }
    }
  }
}
